{
  "name": "acquit-require",
  "version": "0.0.4",
  "description": "Pull examples from your test files into markdown or HTML",
  "main": "index.js",
  "scripts": {
    "docs": "acquit-markdown -i false -p './test/example.test.js' -h './HEADER.md' > README.md && node docs.js",
    "test": "NODE_PATH=../ mocha test/*.test.js"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/vkarpov15/acquit-require.git"
  },
  "peerDependencies": {
    "acquit": ">=0.4.0"
  },
  "devDependencies": {
    "acquit": "0.6.2",
    "acquit-markdown": "0.0.10",
    "mocha": "4.0.0"
  }
}
